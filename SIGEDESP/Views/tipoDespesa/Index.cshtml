@model IEnumerable<tipoDespesaModel>

    <div class="container">
        <div class="d-flex justify-content-between align-items-center mb-5"></div>
        <a class="btn btn-success mx-2" asp-controller="tipoDespesa" asp-action="Cadastrar">Cadastrar Novo Tipo de Despesa</a>
    </div>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">Descrição</th>
        </tr>
    </thead>
    <tbody>
        @if(Model!=null)
        {
            @foreach(var tipoDespesa in Model)
            {
                <tr>
                    <th scope="row">@tipoDespesa.Descricao</th>
                </tr>
            }
        }
    </tbody>
</table>