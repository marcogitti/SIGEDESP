@model IEnumerable<tipoDespesaModel>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">Descrição</th>
        </tr>
    </thead>
    <tbody>
        @if(Model!=null)
        {
            @foreach(var tipoDespesa in Model)
            {
                <tr>
                    <th scope="row">@tipoDespesa.Descricao</th>
                </tr>
            }
        }
    </tbody>
</table>